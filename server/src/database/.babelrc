{
  "presets": [
    [
      "@babel/env",
      {
        "targets": {
          "esmodules": true
        }
      }
    ]
  ],
  "plugins": [
    ["babel-plugin-add-module-exports"],
    [
      "module-resolver",
      {
        "root": ["./"],
        "alias": {
          "~": "./src"
        }
      }
    ]
  ]
}
